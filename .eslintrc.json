{
    "env": {
        "browser": true,
        "es6": true
    },

    "extends": [
        "airbnb",
        "plugin:prettier/recommended" 
    ],

    "plugins": ["prettier"],
    "parser": "babel-eslint",

    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
          }
    },
    "rules": {
            "prettier/prettier": ["error",{
            "endOfLine":"auto"
        }],
        "no-param-reassign": 0,
        "func-names":"off",
        "class-methods-use-this":"off",
        "import/no-extraneous-dependencies": "off"
    }
}


